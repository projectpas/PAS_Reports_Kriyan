  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct 
Doc.Description 'Doc Num',
DocType.Name 'DocType',
 '?'            'CustRef',
 '?'             'EntryDate',
 '?'              'Terms',
 '?'             'DueDate',
CS.annualrevenuepotential            'Amount',
CS.annualquota             'Balance',
CDD.DocDescription             'Description',
CBA.sitename             'Site',
CDD.DocMemo             'Memo'
from CustomerDocumentDetails CDD
LEFT JOIN Customer C ON CDD.CustomerId=C.CustomerId
LEFT JOIN CustomerBillingAddress CBA ON C.CustomerId=CBA.CustomerId
LEFT JOIN CustomerSales CS ON C.CustomerId=CS.CustomerId
LEFT JOIN Document Doc ON C.CustomerId=Doc.Customer
LEFT JOIN DocumentType DocType ON Doc.DocumentTypeId=DocType.DocumentTypeId
where (CDD.MasterCompanyId = 4)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
