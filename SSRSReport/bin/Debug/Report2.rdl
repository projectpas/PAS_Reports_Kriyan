  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SONum">
            <Value>=Parameters!SONum.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT 
                         Customer.Name AS 'CustomerName', Customer.CustomerCode, 
                         SO.SalesOrderNumber AS 'SO Num', SO.OpenDate AS ' OpenDate'
 
FROM            SalesOrder AS SO LEFT OUTER JOIN
                         Customer ON SO.CustomerId = Customer.CustomerId

WHERE        (SO.SalesOrderNumber IN (@SONum)) AND (SO.OpenDate BETWEEN @FromDate AND @ToDate) and (SO.MasterCompanyId IN (4)) </CommandText>
      </Query>
      <Fields>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerCode">
          <DataField>CustomerCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SO_Num">
          <DataField>SO Num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_OpenDate">
          <DataField> OpenDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
